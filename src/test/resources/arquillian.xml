<?xml version="1.0"?>
<arquillian xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://jboss.org/schema/arquillian"
  xsi:schemaLocation="http://jboss.org/schema/arquillian http://www.jboss.org/schema/arquillian/arquillian_1_0.xsd">

  <container qualifier="wls" default="true">
    <configuration>
      <property name="adminUrl">t3://localhost:7001</property>
      <property name="adminUserName">weblogic</property>
      <property name="adminPassword">welcome1</property>
      <property name="target">AdminServer</property>
      <property name="weblogicJarPath">
        {middlewareHome}/wlserver/server/lib/weblogic.jar
      </property>
      <property name="wlHome">
        {middlewareHome}/wlserver
      </property>
    </configuration>
  </container>

</arquillian>
